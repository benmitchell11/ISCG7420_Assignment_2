{
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "frontend/build/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "myproject/wsgi.py/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/build/index.html"
    }
  ],
  "build": {
    "env": {
      "REACT_APP_API_URL": "/api"
    }
  }
}
